{
	"name": "blackdog-configurator-frontend",
	"version": "0.0.5",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:production": "tsc && vite build --mode production",
		"build:development": "tsc && vite build --mode development",
		"build:css": "postcss src/index.css -o src/tailwind.css",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"dev:update:umerx:all": "npm run dev:update:umerx:types && npm run dev:update:umerx:client",
		"dev:update:umerx:types": "npm cache clean --force && npm update @umerx/umerx-blackdog-configurator-types-typescript",
		"dev:update:umerx:client": "npm cache clean --force && npm update @umerx/umerx-blackdog-configurator-client-typescript"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@umerx/umerx-blackdog-configurator-client-typescript": "*",
		"@umerx/umerx-blackdog-configurator-types-typescript": "*",
		"react": "^18.2.0",
		"react-beautiful-dnd": "^13.1.1",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.1",
		"react-select": "^5.7.5"
	},
	"devDependencies": {
		"@types/node": "^20.7.1",
		"@types/react": "^18.2.15",
		"@types/react-beautiful-dnd": "^13.1.4",
		"@types/react-dom": "^18.2.7",
		"@types/react-numeric-input": "^2.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.45.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.33",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.0.2",
		"vite": "^4.4.5",
		"vite-plugin-checker": "^0.6.4"
	}
}
